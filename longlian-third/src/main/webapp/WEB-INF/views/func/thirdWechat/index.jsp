	<script>
		$('.contText').perfectScrollbar();
		$('.allcontText').perfectScrollbar();
		$(".okbtn").click(function(){
			$(".okorno").toggleClass("on");
			if($(".okorno").hasClass("on")){
				sessionStorage.setItem("addClass",1);
				$(".rightLown").attr("href","${requestContext.contextPath}/downloadAttachment");
				$(".ljgzh").addClass("on");
			}else{
				sessionStorage.setItem("addClass",0);
				$(".ljgzh").removeClass("on");
				$(".rightLown").attr("href","javascript:void(0);");
				return;
			}
			$(".ljgzh").click(function(){
				if($(".ljgzh").hasClass("on")){
					goAuthor();
				}else{
					return;
				}
			})
		});
		$(".rightLown").click(function(){
			if($(".okorno").hasClass("on")){
				$(".rightLown").attr("href","${requestContext.contextPath}/downloadAttachment");
			}else{
				alert("请阅读并同意协议规定");
				$(".rightLown").attr("href","javascript:void(0);");
				return;
			}
		})
		$(".yl").click(function(){
			$(".zzc").show();
		})
		$(".clioss").click(function(){
			$(".zzc").hide();
		});
		$(".zzc").bind("click",function(e){  //点击对象
			var target  = $(e.target);//表示当前对象，切记，如果没有e这个参数，即表示整个BODY对象
			if(target.closest(".allcontText").length == 0){    //选择除去对象
				$(".zzc").hide();
			}
		});

		window.onload = function() {
			var stoge = sessionStorage.getItem("addClass");
			if(stoge=='1'){
				$(".okorno").addClass("on");
				$(".ljgzh").addClass("on");
			}

		};



	</script>
